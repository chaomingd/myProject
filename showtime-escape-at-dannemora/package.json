{
		"name": "paidpost-boilerplate",
		"version": "2.3.8",
		"description": "A Paid Post boilerplate project built with scaffolding-next and paidpost-core.",
		"main": "src/index.jsx",
		"private": true,
		"PaidPost": {
			"clientName": "T Brand Studio",
			"clientPath": "t-brand-studio",
			"projectName": "Paid Post Boilerplate",
			"projectPath": "paid-post-boilerplate",
			"clientLink": "http://www.sho.com/order?s_cid=da-dannemora-14284",
			"clientLogoPath": "images/client_logo.png",
			"showClientName": false
		},
		"scripts": {
			"build": "scaffolding -p --env.preset=nyt5_adv.preview --no-env.cloudinary",
			"clean": "rimraf build && rimraf .tmp",
			"lint:script": "node_modules/eslint/bin/eslint.js src/**/*.{js,jsx,json} --fix",
			"lint:style": "node_modules/stylelint/bin/stylelint.js src/**/*.scss --syntax scss && node_modules/stylelint/bin/stylelint.js src/**/*.css",
			"lint": "npm run lint:script && npm run lint:style",
			"precommit": "lint-staged",
			"preview": "preview",
			"prod": "scaffolding -p",
			"serve": "dev-server-bootstrap",
			"staging-link": "staging-link",
			"standalone": "scaffolding -p --env.preset=standalone"
		},
		"repository": {
			"type": "git",
			"url": "git+https://github.com/nytpi/paidpost-boilerplate.git"
		},
		"keywords": [
			"react",
			"scaffolding",
			"paidpost",
			"nytpi",
			"nytimes",
			"nyt"
		],
		"author": "Joseph Kohlmann <joseph.kohlmann@nytimes.com>",
		"license": "ISC",
		"bugs": {
				"url": "https://github.com/nytpi/paidpost-boilerplate/issues"
		},
		"homepage": "https://github.com/nytpi/paidpost-boilerplate#readme",
		"dependencies": {
			"joshua-arrival-listener": "^0.0.11",
			"joshua-platform": "^2.0.4",
			"paidpost-core": "git+https://github.com/nytpi/paidpost-core#semver:^13.3.0",
			"scrollmagic": "^2.0.5",
			"socket.io": "^2.1.1",
			"spaniel": "^2.5.2"
		},
		"peerDependencies": {
			"classnames": "^2.2.5",
			"prop-types": "^15.5.10",
			"react": "^15.6.1"
		},
		"devDependencies": {
			"exports-loader": "^0.7.0",
			"express": "^4.16.4",
			"husky": "^0.14.3",
			"imports-loader": "^0.8.0",
			"lint-staged": "^4.0.3",
			"np": "^3.0.4",
			"nytpi-sass-utils": "git+https://github.com/nytpi/nytpi-sass-utils#semver:^2.3.1",
			"scaffolding-next": "git+https://github.com/nytpi/scaffolding-next#semver:^6.4.2",
			"socket.io-client": "^2.1.1"
		}
}
